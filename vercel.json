{
    "version": 2,
    "builds": [
      {
        "src": "bot.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "bot.js",
        "methods": ["POST"]
      },
      {
        "src": "/health",
        "dest": "bot.js",
        "methods": ["GET"]
      }
    ]
  }